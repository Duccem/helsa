{
  "extends": "@helsa/typescript-config/library.json",
  "compilerOptions": {
    "baseUrl": ".",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@helsa/*": ["../*"],
      "@helsa/ui/*": ["./*"]
    },
    "jsx": "react-jsx"
  },
  "include": ["./*.ts", "./*.tsx"],
  "exclude": ["node_modules"]
}
